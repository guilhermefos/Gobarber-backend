{
  "env": {
      "es2020": true,
      "node": true,
      "jest": true
  },
  "extends": [
      "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
      "no-underscore-dangle": "off",
      "class-methods-use-this": "off",
      "typescript-eslint/no-unused-vars": ["error", {
          "argsIgorePattern": "_"
      }],
      "@typescript-eslint/camelcase": "off"
  }
}